{
  "name": "stylewhere-angularjs",
  "displayName": "Stylewhere + AngularJS",
  "repository": {
    "url": "https://github.com/peterpoe/tmr-angularjstemplate"
  },
  "publisher": "temera",
  "author": {
    "name": "Gregorio Magini",
    "email": "gregorio.magini@temera.it"
  },
  "description": "VSCode enhancementes for Temera Stylewhere",
  "license": "UNLICENSED",
  "version": "0.0.12",
  "icon": "images/icon.jpg",
  "categories": [
    "Other"
  ],
  "qna": false,
  "engines": {
    "vscode": "^1.78.1"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^16.18.29",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.40.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "extensionDependencies": [
    "vscode.html-language-features"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "capabilities": {
    "definitionProvider": true,
    "workspaceSymbolProvider": true
  },
  "contributes": {
    "html": {
      "customData": [
        "./language/angularjstemplate.html-data.json"
      ]
    },
    "languages": [
      {
        "id": "angularjstemplate",
        "aliases": [
          "AngularJS Template",
          "angularjstemplate"
        ],
        "extensions": [
          ".html"
        ],
        "configuration": "./language/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "angularjstemplate",
        "scopeName": "text.html.angularjstemplate",
        "path": "./language/angularjstemplate.tmLanguage.json"
      }
    ],
    "htmlLanguageParticipants": [
      {
        "languageId": "angularjstemplate",
        "autoInsert": true
      }
    ],
    "snippets": [
      {
        "language": "angularjstemplate",
        "path": "./language/angularjstemplate.snippets.json"
      },
      {
        "language": "javascript",
        "path": "./language/javascript.snippets.json"
      }
    ],
    "configurationDefaults": {
      "[angularjstemplate]": {
        "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?"
      },
      "[javascript]": {
        "editor.gotoLocation.multipleDefinitions": "goto"
      }
    }
  },
  "dependencies": {
    "camelcase": "^6.3.0"
  }
}
